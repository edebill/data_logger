<h1>New fahrenheit_temp</h1>

<% form_for(@fahrenheit_temp) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :temp %><br />
    <%= f.text_field :temp %>
  </p>
  <p>
    <%= f.label :source_id %><br />
    <%= f.text_field :source_id %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', fahrenheit_temps_path %>