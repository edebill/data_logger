<h1>Listing time_periods</h1>

<table>
  <tr>
    <th>Source</th>
    <th>Time start</th>
    <th>Time end</th>
  </tr>

<% @time_periods.each do |time_period| %>
  <tr>
    <td><%=h time_period.source_id %></td>
    <td><%=h time_period.time_start %></td>
    <td><%=h time_period.time_end %></td>
    <td><%= link_to 'Show', time_period %></td>
    <td><%= link_to 'Edit', edit_time_period_path(time_period) %></td>
    <td><%= link_to 'Destroy', time_period, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New time_period', new_time_period_path %>